#, python-format
msgid "Dependency group and entry: %(group)s, %(entry)s"
msgstr "Grupul de dependență și intrările  aferente: %(group)s, %(entry)s"

#: views.py:126
#, python-format
msgid "Entry %s not found."
msgstr "Intrarea %s nu a fost găsită."

